<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yy.spring.dao.ExamDao">
	<select id="selectByMid" parameterType="int" resultType="Exam">
	SELECT id,ename,estarttime,eendtime,estate FROM `exam` where mid  =#{mid}
	</select>
</mapper>